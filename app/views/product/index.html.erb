
<div>
    <table>
        <tr>
            <th>ID</th>
            <th>Name</th> 
            <th>Price</th>
            <th>Qty</th>
            <th>Created</th>
            <th>Modified</th>
        </tr>
        <% @products.each do |p| %>
        <tr>
            <td>
                <%= p.id %>
            </td>
            <td>
                <%= p.name %>
            </td>
            <td>
                <%= p.price %>
            </td>
            <td>
                <%= p.qty %>
            </td>
            <td>
                <%= p.created_at %>
            </td>
            <td>
                <%= p.updated_at %>
            </td>
        </tr>
        <% end %>

    </table>
</div
<%= will_paginate @products %>

